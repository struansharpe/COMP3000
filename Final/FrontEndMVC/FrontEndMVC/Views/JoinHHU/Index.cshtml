@model IEnumerable<FrontEndMVC.Models.JoinHHU>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>HHID</th>
        <th>HHName</th>
        <th>Admin User</th>
        <th>FriendlyName</th>
        <th>Restricted Users</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>@item.GetHouseHold.HHID</td>
    <td>@item.GetHouseHold.HHName</td>
    <td>@item.GetHouseHold.AdminUser</td>
    <td>@item.GetUser.FriendlyName</td>
    <td>@item.GetHouseHold.RestrictedUsers</td>
    <td></td>
</tr>
}

</table>
