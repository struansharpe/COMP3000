@model IEnumerable<FrontEndMVC.Models.JoinSSR>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>SSID</th>
        <th>Storage Name</th>
        <th>RID</th>
        <th>Room Name</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>@item.GetStorageSpace.SSID</td>
    <td>@item.GetStorageSpace.StorageName </td>
    <td>@item.GetStorageSpace.RID</td>
    <td>@item.GetRoom.RoomName</td>
    <td> </td>
</tr>
}

</table>
