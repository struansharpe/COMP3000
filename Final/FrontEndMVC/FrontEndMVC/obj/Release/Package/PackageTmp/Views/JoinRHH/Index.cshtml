@model IEnumerable<FrontEndMVC.Models.JoinRHH>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>RID</th>
        <th>Room Name</th>
        <th>HHID</th>
        <th>House Name</th>
        <th></th> 
    </tr>

@foreach (var item in Model) {
<tr>
    <td>@item.GetRoom.RID </td>
    <td>@item.GetRoom.RoomName </td>
    <td>@item.GetRoom.HHID </td>
    <td>@item.GetHouseHold.HHName </td>
    <td> </td>
</tr>
}

</table>
